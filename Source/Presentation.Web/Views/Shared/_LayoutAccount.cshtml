<!DOCTYPE html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js">
<!--<![endif]-->
<head>
    <meta charset="utf-8" />

    <title>@WebResources.NombreSistema</title>

    <meta name="description" content="" />
    <meta name="author" content="" />
    <meta name="robots" content="noindex, nofollow" />
    <meta name="googlebot" content="noindex" />

    <meta name="viewport" content="width=device-width,initial-scale=1" />

    <!-- Icons -->
    <!-- The following icons can be replaced with your own, they are used by desktop and mobile browsers -->
    <link rel="shortcut icon" href="@Url.Content("~/Content/img/icons/favicon.ico")" />
    <link rel="apple-touch-icon" href="@Url.Content("~/Content/img/apple-touch-icon.png")" />
    <link rel="apple-touch-icon" sizes="57x57" href="@Url.Content("~/Content/img/icons/apple-touch-icon-57x57-precomposed.png")" />
    <link rel="apple-touch-icon" sizes="72x72" href="@Url.Content("~/Content/img/icons/apple-touch-icon-72x72-precomposed.png")" />
    <link rel="apple-touch-icon" sizes="114x114" href="@Url.Content("~/Content/img/icons/apple-touch-icon-114x114-precomposed.png")" />
    <link rel="apple-touch-icon-precomposed" href="@Url.Content("~/Content/img/icons/apple-touch-icon-precomposed.png")" />
    <!-- END Icons -->
    <!-- Stylesheets -->
    <!-- The roboto font is included from Google Web Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:400,400italic,700,700italic" />

    <!-- Bootstrap 2.3.2 is included in its original form, unaltered -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/bootstrap.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/animate.css")" type="text/css" />

    <!-- Related styles of various javascript plugins -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/plugins.css")" type="text/css" />

    <!-- The main stylesheet of this template. All Bootstrap overwrites are defined in here -->
    <link rel="stylesheet" href="@Url.Stylesheet("~/Content/css/style.css")" type="text/css" />
    <!-- END Stylesheets -->
    <!-- Modernizr (Browser feature detection library) & Respond.js (Enable responsive CSS code on browsers that don't support them) -->
    <script src="@Url.Content("~/Content/js/modernizr-2.6.2-respond-1.1.0.min.js")" type="text/javascript"></script>

    @RenderSection("styles", required: false)
</head>
<!-- Body -->
<body class="login">
    <!-- Login Container -->
    <div id="login-container">
        <div id="login-logo">
            <div class="dash-tilelogin dash-tile-darklogin clearfix">
                <div class="dash-tile-headerlogin">
                    <img src="@Url.Content("~/Content/img/logoP.png")" class="animated fadeInRight" alt="" />
                </div>
            </div>
        </div>
        @RenderBody()
    </div>
    <!-- END Login Container -->
    <h4 class="labelfooter animated slideInUp">@WebResources.DerechoAutor</h4>

    <!-- Jquery library -->
    <script src="@Url.Content("~/Content/js/jquery-1.10.2.min.js")" type="text/javascript"></script>

    <!-- Bootstrap.js -->
    <script src="@Url.Content("~/Content/js/bootstrap.min.js")" type="text/javascript"></script>

    <!-- Jquery plugins and custom javascript code -->
    <script src="@Url.Content("~/Content/js/plugins.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/pace.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.validate.unobtrusive.bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Script("~/Content/js/webApp.js")" type="text/javascript"></script>
    <script type="text/javascript">
        $(function () {

            var parametros = {
                mensajeEspera: '@Html.Raw(WebResources.MensajeDeEspera)',
                tituloPopupMensaje: '@Html.Raw(WebResources.Informacion)',
                tituloPopupComfirmacion: '',
                mensajePopupConfirmacion: '',
                btnCancelar: '',
                btnAceptar: '@WebResources.Aceptar',
                codigoIdioma: '',
                formatoFecha: '',
            };

            webApp.init(parametros);
        });
    </script>

    @RenderSection("scripts", required: false)
</body>
<!-- END Body -->
</html>
