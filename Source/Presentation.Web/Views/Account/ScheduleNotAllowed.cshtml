@model ScheduleNotAllowedDto
@{
    Layout = "~/Views/Shared/_LayoutAccount.cshtml";
}

<div class="titulo-remesa">@WebResources.HorarioNoPermitido</div>

<div class="form-box-content">

    <div class="alert alert-danger">
        <button type="button" class="close" data-dismiss="alert">×</button>
        @string.Format(WebResources.HorarioMessage, Model.AgenciaNombre, Model.HoraInicio, Model.HoraFin)
    </div>

</div>
<div class="control-group remove-margin clearfix pad">
    <div class="btn-group">
        <a href="@Url.Action("Login")" class="btn btn-small btn-successlogin"><i class="fa fa-repeat"></i> @WebResources.BtnRegresar</a>
    </div>
</div>



